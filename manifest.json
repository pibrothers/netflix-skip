{
    "name": "Netflix - Skip Intro",
    "description": "Auto skip intro in Netflix",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "http://*.netflix.com/*",
                "https://*.netflix.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "icons": {
        "16": "/images/logo16.png",
        "32": "/images/logo32.png",
        "48": "/images/logo48.png",
        "128": "/images/logo128.png"
    }
}